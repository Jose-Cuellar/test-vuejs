{"ast":null,"code":"import axios from \"axios\";\nimport * as XLSX from 'xlsx';\nimport swal from 'sweetalert2';\nexport default {\n  name: 'pruebaAxios',\n\n  data() {\n    return {\n      datos: [],\n      dataToExport: [],\n      // paginación\n      currentPage: 1,\n      pageSize: 5,\n      totalDatos: 0,\n      totalPages: 0,\n      pages: []\n    };\n  },\n\n  methods: {\n    get_data() {\n      var app = this;\n      axios.get(\"https://jsonplaceholder.typicode.com/todos/\").then(res => {\n        app.datos = res.data; // console.log('app.datos: ', app.datos);\n      });\n    },\n\n    downloadExcel() {\n      swal.fire({\n        icon: \"question\",\n        title: \"Descargar reporte\",\n        text: \"¿Deseas descargar el reporte?\",\n        confirmButtonText: \"Sí, Descargar\",\n        confirmButtonColor: \"green\",\n        showCancelButton: true,\n        cancelButtonText: \"No, Cancelar\",\n        cancelButtonColor: \"#d33\",\n        showClass: {\n          popup: 'animate__animated animate__fadeInDown'\n        }\n      }).then(result => {\n        if (result.isConfirmed) {\n          this.datos.forEach(dato => {\n            this.dataToExport.push({\n              Id_Usuario: dato.userId,\n              Id: dato.id,\n              Titulo: dato.title,\n              Completado: dato.completed\n            });\n          });\n          let data = XLSX.utils.json_to_sheet(this.dataToExport);\n          const workbook = XLSX.utils.book_new();\n          const filename = 'axios_get';\n          XLSX.utils.book_append_sheet(workbook, data, filename);\n          XLSX.writeFile(workbook, `${filename}.xlsx`);\n        }\n      });\n    },\n\n    back() {\n      window.history.back();\n    },\n\n    getPages() {\n      this.totalPages = Math.ceil(this.totalDatos / this.pageSize);\n      let pages = [];\n\n      for (let i = 1; i <= this.totalPages; i++) {\n        pages.push(i);\n      }\n\n      return pages;\n    },\n\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n        this.pages = this.getPages(); // actualizar listado de páginas\n      }\n    },\n\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n        this.pages = this.getPages(); // actualizar listado de páginas\n      }\n    },\n\n    changePage(page) {\n      this.currentPage = page;\n    }\n\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.datos.length / (5 * this.perPage));\n    },\n\n    pages() {\n      let start = (this.currentPage - 1) * 5 + 1;\n      let end = start + 4;\n\n      if (end > this.totalPages) {\n        end = this.totalPages;\n      }\n\n      let pagesArray = [];\n\n      for (let i = start; i <= end; i++) {\n        pagesArray.push(i);\n      }\n\n      return pagesArray;\n    },\n\n    paginatedData() {\n      const start = (this.currentPage - 1) * this.perPage;\n      const end = start + this.perPage;\n      return this.datos.slice(start, end);\n    }\n\n  },\n\n  mounted() {\n    this.get_data();\n  }\n\n};","map":{"version":3,"mappings":"AAyEA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAO,KAAKC,IAAZ,MAAsB,MAAtB;AACA,OAAOC,IAAP,MAAiB,aAAjB;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;;EAEbC,IAAI,GAAE;IACJ,OAAM;MACJC,KAAK,EAAE,EADH;MAEJC,YAAY,EAAE,EAFV;MAGJ;MACAC,WAAW,EAAE,CAJT;MAKJC,QAAQ,EAAE,CALN;MAMJC,UAAU,EAAE,CANR;MAOJC,UAAU,EAAE,CAPR;MAQJC,KAAK,EAAE;IARH,CAAN;EAUD,CAbY;;EAcbC,OAAO,EAAC;IACNC,QAAQ,GAAE;MACR,IAAIC,GAAE,GAAI,IAAV;MACAd,KAAK,CAACe,GAAN,CAAU,6CAAV,EAAyDC,IAAzD,CAA+DC,GAAD,IAAS;QACrEH,GAAG,CAACT,KAAJ,GAAYY,GAAG,CAACb,IAAhB,CADqE,CAErE;MACD,CAHD;IAID,CAPK;;IAQNc,aAAa,GAAG;MACdhB,IAAI,CAACiB,IAAL,CAAU;QACRC,IAAI,EAAE,UADE;QAERC,KAAK,EAAE,mBAFC;QAGRC,IAAI,EAAE,+BAHE;QAIRC,iBAAiB,EAAE,eAJX;QAKRC,kBAAkB,EAAE,OALZ;QAMRC,gBAAgB,EAAE,IANV;QAORC,gBAAgB,EAAE,cAPV;QAQRC,iBAAiB,EAAE,MARX;QASRC,SAAS,EAAE;UACTC,KAAK,EAAE;QADE;MATH,CAAV,EAYGb,IAZH,CAYUc,MAAD,IAAY;QACnB,IAAGA,MAAM,CAACC,WAAV,EAAsB;UACpB,KAAK1B,KAAL,CAAW2B,OAAX,CAAoBC,IAAD,IAAU;YAC3B,K